{
  "id": null,
  "uid": null,
    "title": "🚕 NYC Taxi Anomaly Detection Dashboard",
    "description": "Real-time monitoring of NYC taxi trip anomaly detection system",
    "tags": ["anomaly-detection", "nyc-taxi", "ml", "real-time"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
    },
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "🚨 Current Anomaly Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "nyc_taxi_latest_is_anomaly",
            "refId": "A",
            "legendFormat": "Anomaly Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "✅ NORMAL",
                    "color": "green"
                  },
                  "1": {
                    "text": "🚨 ANOMALY",
                    "color": "red"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 0.5}
              ]
            },
            "unit": "none",
            "custom": {
              "displayMode": "basic"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        }
      },
      {
        "id": 2,
        "title": "📊 Total Trips Processed",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "nyc_taxi_total_messages_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "blue", "value": null}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 3,
        "title": "🔥 Total Anomalies Found",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "nyc_taxi_anomalies_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "orange", "value": null}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 4,
        "title": "📈 Anomaly Rate %",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "(nyc_taxi_anomalies_total / nyc_taxi_total_messages_total) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 15}
              ]
            },
            "decimals": 2
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        }
      },
      {
        "id": 5,
        "title": "💰 Current Trip Financial Info",
        "type": "stat",
        "gridPos": {"h": 4, "w": 12, "x": 6, "y": 4},
        "targets": [
          {
            "expr": "nyc_taxi_latest_fare_amount",
            "refId": "A",
            "legendFormat": "Fare Amount"
          },
          {
            "expr": "nyc_taxi_latest_total_amount",
            "refId": "B", 
            "legendFormat": "Total Amount"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "currencyUSD",
            "decimals": 2
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "displayMode": "list",
          "justifyMode": "center"
        }
      },
      {
        "id": 6,
        "title": "🚗 Current Trip Physical Info",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 4},
        "targets": [
          {
            "expr": "nyc_taxi_latest_trip_duration / 60",
            "refId": "A",
            "legendFormat": "Duration (min)"
          },
          {
            "expr": "nyc_taxi_latest_trip_distance", 
            "refId": "B",
            "legendFormat": "Distance (mi)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "decimals": 1
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Duration (min)"},
              "properties": [{"id": "unit", "value": "m"}]
            },
            {
              "matcher": {"id": "byName", "options": "Distance (mi)"},
              "properties": [{"id": "unit", "value": "lengthmi"}]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "vertical",
          "displayMode": "list",
          "justifyMode": "center"
        }
      },
      {
        "id": 7,
        "title": "🚨 Anomaly Detection Timeline",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "nyc_taxi_latest_is_anomaly",
            "refId": "A",
            "legendFormat": "Anomaly Detected"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "stepAfter",
              "lineWidth": 4,
              "fillOpacity": 40,
              "gradientMode": "none",
              "spanNulls": false,
              "pointSize": 10,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"}
            },
            "mappings": [
              {
                "options": {
                  "0": {"text": "Normal", "color": "green"},
                  "1": {"text": "Anomaly", "color": "red"}
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 0.5}
              ]
            },
            "unit": "none",
            "min": -0.1,
            "max": 1.1
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {
            "displayMode": "visible", 
            "placement": "bottom",
            "calcs": ["lastNotNull", "count"]
          }
        }
      },
      {
        "id": 8,
        "title": "🔴 Anomaly Detection Points",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "nyc_taxi_latest_is_anomaly * 1",
            "refId": "A",
            "legendFormat": "🚨 Anomaly Events"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "fixed", "fixedColor": "red"},
            "custom": {
              "drawStyle": "points",
              "lineInterpolation": "linear",
              "lineWidth": 0,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "pointSize": 15,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "Anomaly Events",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false}
            },
            "mappings": [
              {
                "options": {
                  "0": {"text": "", "color": "transparent"},
                  "1": {"text": "🚨 ANOMALY", "color": "red"}
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "transparent", "value": null},
                {"color": "red", "value": 0.5}
              ]
            },
            "unit": "none",
            "min": 0,
            "max": 1.2,
            "displayName": "🚨 Anomaly Detection Events"
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {
            "displayMode": "visible", 
            "placement": "bottom",
            "calcs": ["count"]
          }
        },
        "transformations": [
          {
            "id": "filterFieldsByName",
            "options": {
              "include": {
                "names": ["Time", "🚨 Anomaly Events"]
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "title": "⏱️ Trip Duration Trends",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "nyc_taxi_latest_trip_duration / 60",
            "refId": "A",
            "legendFormat": "Trip Duration (minutes)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 15,
              "pointSize": 6
            },
            "unit": "m",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "orange", "value": 120},
                {"color": "red", "value": 180}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      },
      {
        "id": 10,
        "title": "🛣️ Trip Distance Trends",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "nyc_taxi_latest_trip_distance",
            "refId": "A",
            "legendFormat": "Trip Distance (miles)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 15,
              "pointSize": 6
            },
            "unit": "lengthmi",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 20},
                {"color": "orange", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      },
      {
        "id": 11,
        "title": "💵 Fare Amount Distribution",
        "type": "timeseries", 
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "nyc_taxi_latest_fare_amount",
            "refId": "A",
            "legendFormat": "Fare Amount ($)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear", 
              "lineWidth": 2,
              "fillOpacity": 15,
              "pointSize": 6
            },
            "unit": "currencyUSD",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "orange", "value": 100},
                {"color": "red", "value": 200}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      },
      {
        "id": 12,
        "title": "💰 Total Amount Trends",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30},
        "targets": [
          {
            "expr": "nyc_taxi_latest_total_amount",
            "refId": "A",
            "legendFormat": "Total Amount ($)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 15,
              "pointSize": 6
            },
            "unit": "currencyUSD",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "orange", "value": 120},
                {"color": "red", "value": 250}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      },
      {
        "id": 13,
        "title": "📊 Processing Rate (Messages/minute)",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 38},
        "targets": [
          {
            "expr": "rate(nyc_taxi_total_messages_total[1m]) * 60",
            "refId": "A",
            "legendFormat": "Messages per minute"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 20,
              "pointSize": 5
            },
            "unit": "reqpm"
          }
        },
        "options": {
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      },
      {
        "id": 14,
        "title": "🔥 Anomaly Detection Rate",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 38},
        "targets": [
          {
            "expr": "rate(nyc_taxi_anomalies_total[5m]) * 300",
            "refId": "A",
            "legendFormat": "Anomalies per 5min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 20,
              "pointSize": 5
            },
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "🚨 Anomaly Alerts",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "nyc_taxi_latest_is_anomaly == 1",
          "iconColor": "red",
          "lineColor": "red",
          "titleFormat": "🚨 ANOMALY DETECTED",
          "textFormat": "Anomaly detected in NYC taxi trip data - Check details!",
          "tagKeys": "anomaly,alert",
          "hide": false,
          "showIn": 0,
          "type": "dashboard"
        },
        {
          "name": "🔴 Anomaly Events",
          "datasource": "Prometheus", 
          "enable": true,
          "expr": "changes(nyc_taxi_latest_is_anomaly[30s]) > 0 and nyc_taxi_latest_is_anomaly == 1",
          "iconColor": "red",
          "lineColor": "rgba(255, 0, 0, 0.8)",
          "titleFormat": "🔴 Anomaly Event",
          "textFormat": "Anomaly transition detected",
          "tagKeys": "event",
          "hide": false,
          "showIn": 0,
          "type": "dashboard"
        }
      ]
    }
  } 